<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="false" android:drawable="@drawable/color_switch_bg_off_disabled" />
    <item android:state_enabled="true" android:state_checked="false" android:drawable="@drawable/color_switch_bg_off_normal" />
    <item android:state_enabled="false" android:state_checked="true">
        <bitmap android:src="@drawable/color_switch_bg_on_disabled" android:tint="?colorTintControlNormal" />
    </item>
    <item android:state_enabled="true" android:state_checked="true">
        <bitmap android:src="@drawable/color_switch_bg_on_normal" android:tint="?colorTintControlNormal" />
    </item>
</selector>