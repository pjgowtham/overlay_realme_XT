<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <nine-patch android:src="@drawable/oppo_btn_default_small_normal" android:tint="?colorTintControlPressed" />
    </item>
    <item android:state_enabled="false">
        <nine-patch android:src="@drawable/oppo_btn_default_small_normal" android:tint="?colorTintControlDisabled" />
    </item>
    <item>
        <nine-patch android:src="@drawable/oppo_btn_default_small_normal" android:tint="?colorTintControlNormal" />
    </item>
</selector>